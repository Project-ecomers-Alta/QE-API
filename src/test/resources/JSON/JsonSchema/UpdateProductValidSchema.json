    },
    "data": {
      "type": "object",
      "properties": {
        "user_id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "category": {
          "type": "string"
        },
        "quantity": {
          "type": "integer"
        },
        "price": {
          "type": "integer"
        }
      },
      "required": [
